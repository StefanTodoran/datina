{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "server.py"
    ],
    "rewrites": [
      {
          "source": "/about",
          "destination": "/about.html"
      },
      {
          "source": "/contact",
          "destination": "/contact.html"
      }
    ]
  }
}
